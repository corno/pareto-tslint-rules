{
  "name": "mymodule",
  "version": "0.0.1",
  "description": "whatever",
  "main": "dist/index.js",
  "scripts": {
    "build": "rd /s /q .\\dist && tsc",
    "lint": "tslint --project tsconfig.json",
    "pubMin": "git diff --exit-code && ncu -u --packageFile package.json && npm install && rd /s /q .\\dist && tsc && npm run lint && npm run test && npm version minor && git push && npm publish",
    "pubPatch": "git diff --exit-code && ncu -u --packageFile package.json && npm install && rd /s /q .\\dist && tsc && npm run lint && npm run test && npm version patch && git push && npm publish",
    "test": "cross-env TS_NODE_FILES=true mocha --exit --require ts-node/register --colors test/**/*.ts",
    "u2l": "ncu -u --packageFile package.json && nsi",
    "coverage": "nyc npm run test",
    "test-lint-system-rule": "tslint --test ./test/rules/system",
    "test-lint-pareto-rule": "tslint --test ./test/rules/pareto"
  },
  "keywords": [],
  "author": "corno",
  "license": "ISC",
  "types": "dist/index.d.ts",
  "prepublish": "tsc",
  "repository": {
    "type": "git",
    "url": "https://github.com/corno/mymodule.git"
  },
  "dependencies": {
    "@types/node": "^12.12.14",
    "fountain-pen": "^1.0.0",
    "steroid-writer": "0.1.3",
    "tslint": "^5.20.1",
    "tsutils": "^3.17.1",
    "typescript": "^3.7.2",
    "yarn": "^1.19.2"
  }
}
